<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">
    <fragment
        android:id="@+id/navigation_home"
        android:name="app.pondokcoder.pln_sidak.ui.home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_doc"
        android:name="app.pondokcoder.pln_sidak.ui.document.DocFragment"
        android:label="Document"
        tools:layout="@layout/fragment_document" />

    <fragment
        android:id="@+id/navigation_scan"
        android:name="app.pondokcoder.pln_sidak.ui.history.ScanFragment"
        android:label="Scan"
        tools:layout="@layout/fragment_scan" />

    <fragment
        android:id="@+id/navigation_swa"
        android:name="app.pondokcoder.pln_sidak.ui.swa.SWAFragment"
        android:label="SWA"
        tools:layout="@layout/fragment_swa" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="app.pondokcoder.pln_sidak.ui.profile.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profile" />
</navigation>
